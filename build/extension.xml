<extension xmlns="http://ns.adobe.com/air/extension/3.5">
	<id>com.studiopixmix.anes.inapppurchase</id>
	<versionNumber>0.0.1</versionNumber>
	<platforms>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libInAppPurchaseIosExtension.a</nativeLibrary>
				<initializer>InAppPurchaseIosExtensionInitializer</initializer>
				<finalizer>InAppPurchaseIosExtensionFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		
		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>ANE-InAppPurchase.jar</nativeLibrary>
				<initializer>com.studiopixmix.anes.inapppurchase.InAppPurchaseExtension</initializer>
				<finalizer>com.studiopixmix.anes.inapppurchase.InAppPurchaseExtension</finalizer>
			</applicationDeployment>
		</platform>
		
		<platform name="Android-x86">
			<applicationDeployment>
				<nativeLibrary>ANE-InAppPurchase.jar</nativeLibrary>
				<initializer>com.studiopixmix.anes.inapppurchase.InAppPurchaseExtension</initializer>
				<finalizer>com.studiopixmix.anes.inapppurchase.InAppPurchaseExtension</finalizer>
			</applicationDeployment>
		</platform>
		
		<platform name="default">
			<applicationDeployment/>
		</platform>
	</platforms>
</extension>
